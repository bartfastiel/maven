<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="template"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <fileSets>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>runner/felix</directory>
      <includes>
        <include>**/*.properties</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>runner/configurations</directory>
      <includes>
        <include>**/*.properties</include>
        <include>**/*.txt</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>runner</directory>
      <includes>
        <include>**/*.bat</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>eclipse.launchers</directory>
      <includes>
        <include>**/*.launch</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>runner/configurations</directory>
      <includes>
        <include>**/*.id</include>
        <include>**/*.</include>
        <include>**/*.dat</include>
        <include>**/*.hash</include>
        <include>**/*.xsd</include>
        <include>**/*.prop</include>
        <include>**/*.space</include>
        <include>**/*.key</include>
        <include>**/*.alloc</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>pax.configuration</include>
        <include>Run.bat</include>
      </includes>
    </fileSet>
  </fileSets>
  <requiredProperties>
  	<requiredProperty key="platform-version">
  		<defaultValue>${platform.version}</defaultValue>
  	</requiredProperty>
  </requiredProperties>
</archetype-descriptor>
